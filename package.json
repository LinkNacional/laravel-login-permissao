{
    "private": true,
    "scripts": {
        "dev": "cd quasarapp && quasar build && cd ../ && npm run development && npm run serve",
        "prod": "cd quasarapp && quasar build && cd ../ && npm run production && npm run serve",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "serve": "php artisan serve",
        "production": "mix --production",
        "test": "php artisan test"
    },
    "devDependencies": {
        "axios": "^0.21",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "prettier": "^2.2.1"
    }
}