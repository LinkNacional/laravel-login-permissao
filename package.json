{
    "private": true,
    "scripts": {
        "dev": "cd quasarapp && quasar build && cd ../ && npm run development && npm run serve",
        "prod": "cd quasarapp && quasar build && cd ../ && npm run production && npm run serve",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "serve": "php artisan serve",
        "production": "mix --production",
        "test": "php artisan test",
        "websockets":"php artisan websockets:serve"
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-echo": "^1.10.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "pusher-js": "^7.0.3"
    }
}
